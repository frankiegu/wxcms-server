{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "minimum-stability":"dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.0.0",
        "actuallymab/laravel-comment": "^1.0",
        "encore/laravel-admin": "v1.6.13",
        "fideloper/proxy": "~3.3",
        "intervention/image": "^2.4",
        "laravel-admin-ext/cropper": "^1.0",
        "laravel-admin-ext/helpers": "^1.1",
        "laravel-admin-ext/redis-manager": "^1.0",
        "laravel-admin-ext/simplemde": "^1.0",
        "laravel/framework": "5.5.*",
        "laravel/scout": "^7.1",
        "laravel/tinker": "~1.0",
        "laravelnews/laravel-twbs4": "^1.3",
        "lcobucci/jwt": "^3.2",
        "overtrue/laravel-filesystem-qiniu": "^1.0",
        "overtrue/laravel-follow": "^1.1",
        "overtrue/laravel-wechat": "~4.0",
        "predis/predis": "^1.1",
        "prettus/l5-repository": "^2.6",
        "rachidlaasri/laravel-installer": "^4.0",
        "spatie/laravel-activitylog": "^3.2",
        "spatie/laravel-tags": "^2.1",
        "tymon/jwt-auth": "dev-develop",
        "vinkla/hashids": "3.3",
        "yab/laravel-scout-mysql-driver": "^2.3"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
